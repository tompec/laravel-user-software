{
    "name": "tompec/laravel-user-software",
    "description": "A Laravel package to log users' software for troubleshooting purposes.",
    "keywords": [
        "laravel",
        "user",
        "software"
    ],
    "homepage": "https://github.com/tompec/laravel-user-software",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Petracco",
            "email": "git@mail.tompec.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tompec\\UserSoftware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tompec\\UserSoftware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tompec\\UserSoftware\\UserSoftwareServiceProvider"
            ]
        }
    }
}
